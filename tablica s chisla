package ddddd;

import java.util.Scanner;

public class d78 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();

for(int i=1;i<=n;i++) 
{
int br=1;
int j=i;
boolean p=false;
while(br<=n) {
	if(j==n)p=true;
	System.out.print(j+" ");
	if(p==false)j++;
	else j--;
	br++;

}
	System.out.println();
}
	}

}
